{
  "name": "react-component-explorer",
  "version": "1.0.0",
  "description": "react component explorer",
  "scripts": {
    "test": "eslint lib/ spec/ && ./node_modules/karma/bin/karma start karma.conf.js",
    "watch-test": "./node_modules/karma/bin/karma start karma.conf.js --auto-watch --no-single-run",
    "build": "make build",
    "watch": "webpack -p --watch",
    "rename-css-bower-dependencies": "find bower_components -name '*.css' | sed -e 'p;s/.css$/.scss/' | xargs -n2 mv",
    "start": "webpack-dev-server --inline --hot"
  },
  "author": "",
  "license": "BSD",
  "keywords": [
    "react",
    "reactjs",
    "react-component"
  ],
  "main": "dist/rce.js",
  "devDependencies": {
    "babel-core": "^6.3.21",
    "babel-eslint": "4.1.6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "0.23.0",
    "es5-shim": "4.3.1",
    "eslint": "1.10.1",
    "eslint-plugin-react": "3.10.0",
    "immutable": "^3.7.5",
    "jasmine": "~2.3.2",
    "jasmine-core": "~2.3.4",
    "node-sass": "3.4.2",
    "phantomjs-prebuilt": "~2.1.3",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "webpack": "1.12.9",
    "webpack-dev-server": "~1.10.1"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "babel-runtime": "6.2.0",
    "react-layout-pane": "^0.1.10"
  },
  "peerDependencies": {
    "jscodeshift": "^0.3.11",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "recast": "^0.10.39"
  }
}
