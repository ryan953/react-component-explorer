{
  "name": "react-component-explorer",
  "version": "1.0.0",
  "description": "react component explorer",
  "scripts": {
    "test": "eslint lib/ spec/ && ./node_modules/karma/bin/karma start karma.conf.js",
    "watch-test": "./node_modules/karma/bin/karma start karma.conf.js --auto-watch --no-single-run",
    "build": "webpack -p",
    "watch": "webpack -p --watch",
    "rename-css-bower-dependencies": "find bower_components -name '*.css' | sed -e 'p;s/.css$/.scss/' | xargs -n2 mv",
    "start": "webpack-dev-server --inline --hot",
    "postinstall": "bower install"
  },
  "author": "",
  "license": "BSD",
  "keywords": [
    "react",
    "reactjs",
    "react-component"
  ],
  "main": "dist/main.js",
  "devDependencies": {
    "babel-core": "6.2.1",
    "babel-eslint": "4.1.6",
    "babel-loader": "6.2.0",
    "css-loader": "0.23.0",
    "es5-shim": "4.3.1",
    "eslint": "1.10.1",
    "eslint-plugin-react": "3.10.0",
    "immutable": "^3.7.5",
    "jasmine": "~2.3.2",
    "jasmine-core": "~2.3.4",
    "karma": "~0.13.9",
    "karma-jasmine": "~0.3.6",
    "karma-phantomjs-launcher": "~0.2.1",
    "karma-sourcemap-loader": "~0.3.5",
    "karma-webpack": "~1.7.0",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "webpack": ".12.9",
    "webpack-dev-server": "~1.10.1"
  },
  "dependencies": {
    "babel-runtime": "6.2.0",
    "react": "0.14.3"
  }
}
